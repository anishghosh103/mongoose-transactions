{
  "name": "mongoose-transactions",
  "version": "1.1.3",
  "description": "Atomicity and Transactions for mongoose",
  "main": "index.js",
  "scripts": {
    "lint": "eslint -c .eslintrc.js --fix --ext .ts 'src/**/*.ts' '__tests__/**/*.ts'",
    "lint:fix": "eslint --fix",
    "pretest": "npm run lint",
    "test": "jest --forceExit --runInBand",
    "build": "tsc -w false"
  },
  "pre-commit": [
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daton89-topperblues/mongoose-transactions.git"
  },
  "keywords": [
    "mongoose",
    "transactions"
  ],
  "author": "Nicola Bonavita @topperblues, Antonio D'Angelo @daton89",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/daton89-topperblues/mongoose-transactions/issues"
  },
  "homepage": "https://github.com/daton89-topperblues/mongoose-transactions#readme",
  "peerDependencies": {
    "mongoose": ">=7.0.0"
  },
  "devDependencies": {
    "@types/core-js": "^2.5.3",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.3",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "core-js": "^3.6.4",
    "eslint": "^8.57.0",
    "eslint-plugin-jsdoc": "^48.7.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jest": "^29.7.0",
    "mongoose": "^8.5.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.0.2",
    "ts-jest": "^29.2.2",
    "typescript": "^5.5.2"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$",
    "verbose": true,
    "notify": false,
    "bail": false
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "tabWidth": 4
  }
}
